server {
    listen 443 ssl;
  http2 on;
    listen 443 quic;

    add_header Alt-Svc 'h3=":$server_port"; ma=86400';
	add_header  Strict-Transport-Security "max-age=31536000; includeSubDomains; preload";

    server_name goaccess.shutupandtakemy.codes;

    location / {
        root /var/www/report;
        access_log /dev/null;
        error_log /dev/null;
    }
 
    location = /favicon.ico {
	return 204;
        access_log /dev/null;
        log_not_found off;
    }
}
