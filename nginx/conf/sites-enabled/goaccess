server {
    listen 443 ssl http2;
    listen 443 quic;

    add_header Alt-Svc 'h3=":$server_port"; ma=86400';

    server_name goaccess.shutupandtakemy.codes;

    location / {
        root /var/www/report;
        access_log /dev/null;
        error_log /dev/null;
    }
 
    location = /favicon.ico {
	return 204;
        access_log /dev/null;
        log_not_found off;
    }
}
