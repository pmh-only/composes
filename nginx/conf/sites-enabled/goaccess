server {
	include /etc/nginx/commons.conf;
	include /etc/nginx/ssl-shutupandtakemycodes.conf;

    server_name goaccess.shutupandtakemy.codes;

    location / {
        root /var/www/report;
        access_log /dev/null;
        error_log /dev/null;
    }
 
    location = /favicon.ico {
	return 204;
        access_log /dev/null;
        log_not_found off;
    }
}
