server {
	listen 80 default_server;
	server_name _;

	location / {
		return 301 https://$host$request_uri;
	}
}

server {
	include /etc/nginx/commons.conf;
	include /etc/nginx/ssl-shutupandtakemycodes.conf;

	root /var/www/notfounddns;

	index index.html;

	server_name *.shutupandtakemy.codes;

	location / {
		try_files $uri /index.html =404;
	}
}

server {
	include /etc/nginx/commons.conf;
	include /etc/nginx/ssl-pmhcodes.conf;

	root /var/www/notfounddns;

	index index.html;

	server_name *.pmh.codes;

	location / {
		try_files $uri /index.html =404;
	}
}

